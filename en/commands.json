{
    "adminmodule": "Administrative Commands",
    "blockxp": {
        "summary": "Prevent a role from gaining chat XP.",
        "usage": "`h!blockxp`\n`h!blockxp <add | remove> <role>`"
    },
    "export": {
        "summary": "Exports your server's settings as a json file. **Requires Server Owner**",
        "usage": "`h!export`"
    },
    "joinmessage": {
        "summary": "Add or remove a join message. `{user}` to mention the user. `{guild}` to print server name.",
        "usage": "`h!joinmessage`\n`h!joinmessage add <message>`\n`h!joinmessage remove`"
    },
    "leavemessage": {
        "summary": "Add or remove a leave message. `{user}` to print user's name. `{guild}` to print server name.",
        "usage": "`h!leavemessage`\n`h!leavemessage add <message>`\n`h!leavemessage remove`"
    },
    "levelrole": {
        "summary": "Add or remove a leveled role",
        "usage": "`h!levelrole`\n`h!levelrole <add | remove> <role> <level>"
    },
    "messagelog": {
        "summary": "Retrieves a message from the deleted message log (if enabled).",
        "usage": "`h!messagelog <message number>`\n`h!messagelog <user> <message number>`"
    },
    "selfrolelist": {
        "summary": "Add or remove a self assignable role",
        "usage": "`h!selfrolelist <add | remove> <role>`"
    },
    "setting": {
        "summary": "Adjust settings for your server.",
        "usage": "`h!setting`\n`h!setting <setting> <value>`"
    },
    "subreddit": {
        "summary": "Add or remove a subreddit.",
